{
  "name": "req69775",
  "version": "0.1.0",
  "description": "A quickstart for bootstrapping CDK projects in Typescript",
  "license": "UNLICENSED",
  "bin": "bin/app.js",
  "scripts": {
    "awslint": "npx awslint",
    "build": "tsc && node build.config.mjs",
    "build:dev": "tsc && node build.config.mjs --dev",
    "cdk": "cdk",
    "cfg-check": "cfg-check --stacks $(cdk list | sed 'N;s/\\n/ /')",
    "clean": "rimraf dist/ cdk.out/",
    "debug:example-lambda": "sam-beta-cdk local invoke abc-quickstart-cdk-typescript-mystack/ExampleLambda -e test/events/example-lambda.json --debug-port 5858 --debug-args '--enable-source-maps' --env-vars env.json",
    "docker-shell": "docker run -it -v ${PWD}:/data/jenkins/workspace/quickstart-cdk-typescript_develop -w='/data/jenkins/workspace/quickstart-cdk-typescript_develop' node:12-alpine /bin/sh",
    "lint": "eslint \"bin/**\" \"lib/**\" \"test/**\"",
    "test": "jest --coverage",
    "watch": "run-p watch:esbuild watch:tsc",
    "watch:esbuild": "node build.config.mjs --watch --dev",
    "watch:tsc": "tsc -w"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.144.0",
    "@aws-cdk/aws-cloudfront": "1.144.0",
    "@aws-cdk/aws-cloudwatch-actions": "1.144.0",
    "@aws-cdk/aws-dynamodb": "1.144.0",
    "@aws-cdk/aws-kinesisfirehose": "1.144.0",
    "@aws-cdk/aws-lambda": "1.144.0",
    "@aws-cdk/aws-s3-deployment": "1.144.0",
    "@aws-cdk/aws-wafv2": "1.144.0",
    "@aws-cdk/core": "1.144.0",
    "@aws-sdk/client-dynamodb": "^3.53.0",
    "@aws-sdk/client-s3": "^3.53.1",
    "@aws-sdk/lib-dynamodb": "^3.53.0",
    "@aws-sdk/util-dynamodb": "^3.53.0",
    "@moo-cdk/core": "15.2.0",
    "@moo-cdk/custom-resources": "15.2.0",
    "@moo-cdk/moo-apigateway": "15.2.0",
    "@moo-cdk/moo-certificatemanager": "15.2.0",
    "@moo-cdk/moo-cloudfront": "15.2.0",
    "@moo-cdk/moo-cloudfront-patterns": "15.2.0",
    "@moo-cdk/moo-cloudwatch": "15.2.0",
    "@moo-cdk/moo-dns": "15.2.0",
    "@moo-cdk/moo-dynamodb": "15.2.0",
