    "@moo-cdk/moo-kms": "15.2.0",
    "@moo-cdk/moo-lambda": "15.2.0",
    "@moo-cdk/moo-logs": "15.2.0",
    "@moo-cdk/moo-s3": "15.2.0",
    "@moo-cdk/moo-sqs": "15.2.0",
    "@moo-cdk/moo-wafv2": "15.2.0",
    "constructs": "3.3.217",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.144.0",
